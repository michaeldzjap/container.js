{
  "name": "container.js",
  "version": "0.0.1",
  "description": "The Laravel container re-written in TypeScript",
  "main": "dist/container.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "build": "./node_modules/.bin/rollup --config",
    "watch": "./node_modules/.bin/rollup --config --watch",
    "clean": "rm -rf ./dist"
  },
  "keywords": [
    "laravel",
    "dependency injection",
    "inversion of control",
    "di",
    "ioc",
    "javascript",
    "typescript"
  ],
  "author": "Michael Dzjaparidze",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "eslint": "^5.10.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^23.6.0",
    "rand-seed": "^0.1.0",
    "rollup": "^0.68.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "ts-jest": "^23.10.5",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2"
  },
  "dependencies": {
    "cherow": "^1.6.9",
    "reflect-metadata": "^0.1.12"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^@helpers/(.*)$": "<rootDir>/__tests__/helpers/$1",
      "^@src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/helpers/",
      "<rootDir>/__tests__/setup.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  }
}
